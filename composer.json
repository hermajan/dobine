{
	"name": "hermajan/dobine",
	"description": "Doctrine extensions.",
	"type": "library",
	"license": "MIT",
	"homepage": "https://hermajan.net/dobine",
	"support": {
		"issues": "https://github.com/hermajan/dobine/issues"
	},
	"autoload": {
		"psr-4": {
			"Dobine\\": "src"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon"
		],
		"tester": [
			"php ./vendor/nette/tester/src/tester.php -C -s tests"
		]
	},
	"repositories": {
		"jfsenechal_doctrine_behaviors": {
			"type": "vcs",
			"url": "git@github.com:jfsenechal/DoctrineBehaviors.git"
		}
	},
	"require": {
		"php": ">=7.4",
		"doctrine/orm": "^2.2"
	},
	"require-dev": {
		"knplabs/doctrine-behaviors": "dev-sf7.0 as 2.6.3",
		"nette/neon": "^3.3",
		"nette/tester": "^2.4",
		"nette/utils": "^4.0",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-doctrine": "^2.0",
		"phpstan/phpstan-nette": "^2.0",
		"vlucas/phpdotenv": "^2.5"
	},
	"suggest": {
		"knplabs/doctrine-behaviors": "For translating entities.",
		"nette/neon": "For parsing connection parameters from .neon files.",
		"nette/utils": "For using with Nette.",
		"vlucas/phpdotenv": "For loading connection environment from .env file."
	}
}
